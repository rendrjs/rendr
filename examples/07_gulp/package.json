{
  "name": "rendr-example",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-karma",
    "test-unit": "mocha --ui bdd --reporter spec ./test/app --recursive --require ./test/helper.js",
    "test-karma": "gulp compile && karma start test/karma.config.js --single-run",
    "start": "node index.js",
    "postinstall": "test -f ../../package.json && npm install ../../ || echo",
    "postupdate": "test -f ../../package.json && npm install ../../ || echo"
  },
  "dependencies": {
    "express": "~3",
    "underscore": "~1.5.2",
    "rendr-handlebars": "0.2.0",
    "rendr": "0.5.0"
  },
  "devDependencies": {
    "gulp": "~3.5.5",
    "gulp-util": "~2.2.14",
    "gulp-plumber": "~0.5.6",
    "gulp-concat": "~2.2.0",
    "gulp-wrap": "~0.3.0",
    "gulp-handlebars": "~1.0.2",
    "gulp-stylus": "~0.1.0",
    "gulp-nodemon": "~1.0.2",
    "glob": "~3.2.9",
    "browserify": "~3.32.0",
    "vinyl-source-stream": "~0.1.1",
    "mocha": "~1.9.0",
    "chai": "~1.8.1",
    "karma": "~0.10.4",
    "karma-chrome-launcher": "~0.1.0",
    "karma-mocha": "~0.1.0"
  },
  "engines": {
    "node": ">=0.8"
  },
  "private": true
}
