{
  "name": "rendr",
  "version": "0.0.1",
  "description": "A small web framework that lets you use Node and Backbone to render on both sides.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --ui bdd --reporter spec --ignore-leaks ./test/**/*.test.* --timeout 6000 --require coffee-script",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/spikebrehm/rendr.git"
  },
  "dependencies": {
    "express": "3.x",
    "coffee-script": "*",
    "underscore": "1.4.2",
    "backbone": "0.9.2",
