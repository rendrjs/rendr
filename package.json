{
  "name": "rendr",
  "version": "0.0.1",
  "description": "A small web framework that lets you use Node and Backbone to render on both sides.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --ui bdd --reporter spec --ignore-leaks $(find ./test -name *.test.*) --timeout 6000 --require coffee-script",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/spikebrehm/rendr.git"
  },
  "dependencies": {
    "coffee-script": "*",
    "underscore": "1.4.2",
    "backbone": "0.9.2",
    "handlebars": "1.0.7",
    "async":"0.1.22",
    "node-polyglot": "0.1.0",
    "qs": "0.5.1"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "sinon": "1.4.2",
    "expect.js": "0.1.2"
  },
  "keywords": [
    "Backbone",
    "view",
    "web",
    "framework"
  ],
