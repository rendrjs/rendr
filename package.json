    "hbs":"1.0.5",
    "stylus":"~> 0.29",
    "stitch": "0.3.3",
    "async":"0.1.22",
    "request":"2.11.1",
    "MD5": "1.0.1",
    "mkdirp": "*",
    "walk": "2.2.1",
    "winston":"0.6.2",
    "node-polyglot": "*",
    "qs": "0.5.1"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "sinon": "1.4.2",
    "expect.js": "0.1.2"
